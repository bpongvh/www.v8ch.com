{
  "extends": "tslint:latest",
  "rules": {
    "interface-name": false,
    "max-line-length": [
      true,
      120
    ],
    "no-console": false,
    "no-var-requires": false,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-interfaces"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case",
      "ban-keywords"
    ]
  },
  "rulesDirectory": []
}
