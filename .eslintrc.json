{
  "extends": ["airbnb-base", "plugin:vue/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "document": false
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": 0
  }
}
